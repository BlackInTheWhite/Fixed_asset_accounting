	<queryString>
		<![CDATA[SELECT
     changes_in_cost.`document_name` AS changes_in_cost_document_name,
     changes_in_cost.`document_data` AS changes_in_cost_document_data,
     changes_in_cost.`document_number` AS changes_in_cost_document_number,
     changes_in_cost.`amount_of_expenses` AS changes_in_cost_amount_of_expenses,
     changes_cost_operation_type_directory.`operation_type_name` AS changes_cost_operation_type_directory_operation_type_name,
     inventory_card.`card_number` AS inventory_card_card_number,
     inventory_card.`object_name` AS inventory_card_object_name
FROM
     `inventory_card` inventory_card INNER JOIN `changes_in_cost` changes_in_cost ON inventory_card.`id_inventory_card` = changes_in_cost.`card_number`
     INNER JOIN `changes_cost_operation_type_directory` changes_cost_operation_type_directory ON changes_in_cost.`operation_type` = changes_cost_operation_type_directory.`id_operation_type`
WHERE changes_in_cost.`document_data` >= $P{DATE_START}
AND changes_in_cost.`document_data`<= $P{DATE_END}]]>
	</queryString>
